{
  "name": "ai-chinese-chess",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare-env": "python3 -m venv .venv && source .venv/bin/activate",
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload --port 8000",
    "next-dev": "next dev --turbopack",
    "dev": "concurrently \"$npm_execpath run next-dev\" \"$npm_execpath run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.1.6",
    "@nextui-org/react": "^2.6.11",
    "@nextui-org/system": "^2.4.6",
    "@nextui-org/theme": "^2.4.5",
    "@types/mdx": "^2.0.13",
    "canvas-confetti": "^1.9.3",
    "ffish": "^0.7.6",
    "ffish-es6": "^0.7.6",
    "motion": "^11.18.0",
    "next": "^15.1.4",
    "next-themes": "^0.4.4",
    "openai": "^4.28.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.1",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.1",
    "readline-sync": "^1.4.10",
    "remark-gfm": "^4.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/readline-sync": "^1.4.8",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
