{
  "name": "ai-chinese-chess",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare-env": "python3 -m venv .venv && source .venv/bin/activate",
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload --port 8000",
    "next-dev": "next dev --turbopack",
    "dev": "concurrently \"$npm_execpath run next-dev\" \"$npm_execpath run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.10.0",
    "@mui/material": "^5.15.6",
    "axios": "^1.7.9",
    "chess.js": "^1.0.0-beta.8",
    "clsx": "^2.1.1",
    "motion": "^11.17.0",
    "next": "^15.1.4",
    "openai": "^4.28.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
